{
  "defaultSeverity": "warning",
  "extends": [
    "tslint-eslint-rules",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "jsRules": {
    "trailing-comma": [true, {
      "multiline": "",
      "singleline": "always"
    }]
  },
  "rules": {
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false
  }
}
